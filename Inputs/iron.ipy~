def Update(Input,Output):
	"try:"
		if hasattr(Input,'Yaw'):
			Output.Yaw.Result = Input.Yaw
		if hasattr(Input,'YKp'):
			Output.Yaw.Kp = Input.YKp
		if hasattr(Input,'YKi'):
			Output.Yaw.Ki = Input.YKi
		if hasattr(Input,'YKd'):
			Output.Yaw.Kd = Input.YKd
	"except:"
		"pass"
	

	try:
		if hasattr(Input,'Roll'):
			Output.Roll.Result = Input.Roll
		if hasattr(Input,'RKp'):
			Output.Roll.Kp = Input.RKp
		if hasattr(Input,'RKi'):
			Output.Roll.Ki = Input.RKi
		if hasattr(Input,'RKd'):
			Output.Roll.Kd = Input.RKd
	except:
		pass
	
	try:
		if hasattr(Input,'Pitch'):
			Output.Pitch.Result = Input.Pitch
		if hasattr(Input,'PKp'):
			Output.Pitch.Kp = Input.PKp
		if hasattr(Input,'PKi'):
			Output.Pitch.Ki = Input.PKi
		if hasattr(Input,'PKd'):
			Output.Pitch.Kd = Input.PKd
	except:
		pass
	
	try:
		if hasattr(Input,'Altitude'):
			Output.Altitude.Result = Input.Altitude
		if hasattr(Input,'AKp'):
			Output.Altitude.Kp = Input.AKp
		if hasattr(Input,'AKi'):
			Output.Altitude.Ki = Input.AKi
		if hasattr(Input,'AKd'):
			Output.Altitude.Kd = Input.AKd
	except:
		pass
	

	
